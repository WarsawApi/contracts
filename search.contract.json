{
  "schema": {
    "request": {
      "path": "/search",
      "method": "get",
      "params": [
        {
          "name": "preferences",
          "type": "object",
          "embedded": [
            {
              "name": "nearby",
              "type": "object",
              "embedded": [
                {
                  "name": "phrase",
                  "type": "string"
                },
                {
                  "name": "location",
                  "type": "object",
                  "embedded": [
                    {
                      "name": "latitude",
                      "type": "string"
                    },
                    {
                      "name": "longitude",
                      "type": "string"
                    }
                  ]
                }
              ]
            },
            {
              "name": "transportation",
              "type": "object",
              "embedded": [
                {
                  "name": "subway",
                  "type": "boolean"
                },
                {
                  "name": "tramway",
                  "type": "boolean"
                },
                {
                  "name": "subway",
                  "type": "boolean"
                }
              ]
            }
          ]
        },
        {
          "name": "filters",
          "type": "object",
          "embedded": [
            {
              "name": "price",
              "type": "object",
              "embedded": [
                {
                  "name": "min",
                  "type": "string"
                },
                {
                  "name": "max",
                  "type": "string"
                }
              ]
            }
          ]
        }
      ]
    },
    "response": {
      "body": {
        "type": "object",
        "embedded": [
          {
            "name": "items",
            "type": "array",
            "embedded": [
              {
                "type": "object",
                "embedded": [
                  {
                    "name": "id",
                    "type": "string"
                  },
                  {
                    "name": "latitude",
                    "type": "string"
                  },
                  {
                    "name": "longitude",
                    "type": "string"
                  },
                  {
                    "name": "address",
                    "type": "string"
                  }
                ]
              }
            ]
          }
        ]
      }
    }
  },
  "examples": [
    {
      "request": {
        "params": {
          "school": 3,
          "metro": 5
        }
      },
      "response": {
        "body_path": "/search/search_example.json"
      }
    }
  ]
}
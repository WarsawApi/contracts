{
  "schema": {
    "request": {
      "path": "/search",
      "method": "get",
      "params": [
        {
          "name": "school",
          "type": "number"
        },
        {
          "name": "metro",
          "type": "number"
        }
      ]
    },
    "response": {
      "body": {
        "type": "object",
        "embedded": [
          {
            "name": "items",
            "type": "array",
            "embedded": [
              {
                "type": "object",
                "embedded": [
                  {
                    "name": "id",
                    "type": "string"
                  },
                  {
                    "name": "latitude",
                    "type": "string"
                  },
                  {
                    "name": "longitude",
                    "type": "string"
                  },
                  {
                    "name": "address",
                    "type": "string"
                  }
                ]
              }
            ]
          }
        ]
      }
    }
  },
  "examples": [
    {
      "request": {
        "params": {
          "school": 3,
          "metro": 5
        }
      },
      "response": {
        "body_path": "/search/search_example.json"
      }
    }
  ]
}